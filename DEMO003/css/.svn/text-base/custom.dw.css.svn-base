@charset "UTF-8";
/*重置body*/
html,
body {
  height: 100%;
}

body {
  font-family: "PingFang SC Regular";
  font-size: 18px;
  background: #fff;
}

/*健康卫士主页 healthGuard.html start*/
.dw-health {
  background-image: url(../image/banner01.png);
