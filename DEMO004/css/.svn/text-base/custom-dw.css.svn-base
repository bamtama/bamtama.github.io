@charset "UTF-8";
/*默认单行缩略*/
.dw-annoucement .new-ann-wrap a p.title, .tip-wrap .mui-table-view-cell a .title {
  overflow: hidden;
  vertical-align: middle;
  text-overflow: ellipsis;
}

/*默认缩略后添加变为无缩略*/
.mui-table-view .mui-table-view-cell .title-break {
  white-space: normal;
  word-break: break-all;
}

/*默认缩略后添加改为多行文本缩略*/
/*guideList.html start*/
.mui-table-view .mui-table-view-cell a.dw-flex {
  display: flex;
}

.mui-table-view .mui-table-view-cell a.dw-flex .title {
  flex: auto;
}

.mui-table-view .mui-table-view-cell a.dw-flex .titleIcon {
  float: right;
  flex: none;
}

/*guideList.html end*/
/*salatyPage.html,attendencePage.html公共样式   start*/
.dw-homebarpage-list {
  top: 44px;
}

.dw-homebarpage-list .iconfont {
  line-height: 44px;
  color: #1B98E6;
}

.dw-homebarpage-list .mui-card {
  margin: 10px 12px;
  border-radius: 4px;
  box-shadow: none;
}

.dw-homebarpage-list .mui-card:first-child {
  margin-top: 5px;
}

.dw-homebarpage-list .mui-table-view {
  padding: 8px 15px 9px 24px;
}

.dw-homebarpage-list .mui-table-view-cell {
  padding: 14px 9px 14px 0px;
  font-family: "PingFang SC Regular";
  font-size: 18px;
}

.dw-homebarpage-list .mui-table-view-cell:after {
  left: 0;
  right: 9px;
  height: 2px;
}

.dw-homebarpage-list .mui-table-view-cell > a:not(.mui-btn) {
  margin: -12px 0px;
  margin-right: -8px;
  padding-left: 15px;
  color: #666666;
}

.dw-homebarpage-list .mui-navigate-right:after {
  right: -3px;
  font-weight: bold;
}

.dw-homebarpage-list li.dw-li-text {
  padding: 13px 9px 13px 7px;
  line-height: 18px;
  font-size: 21px;
  color: #292929;
}

.dw-homebarpage-list li.dw-li-text span {
  font-family: "PingFang SC Medium";
  font-size: 24px;
  color: #ff6612;
}

.dw-homebarpage-list li.dw-attendence-li {
  padding-left: 13px;
}

.dw-homebarpage-list .dw-plus:after {
  content: "\e468";
}

.dw-homebarpage-list .icon-pin {
  font-size: 15px;
  position: absolute;
  top: 14px;
  left: 6px;
}

/*salatyPage.html,attendencePage.html公共样式   end*/
/*attendencePage.html,applyPage.html 相同独立样式 start*/
.dw-attendence-page .icon-pin,
.dw-apply-page .icon-pin {
  top: 9px;
}

/*attendencePage.html,applyPage.html 相同独立样式 end*/
/*salaryWelfareTaxQuety.html 独立样式 start*/
.mui-table-view > .dw-welfare-tax-list {
  padding: 20px 18px;
}

/*salaryWelfareTaxQuety.html 独立样式 end*/
/*apply-proofOfIncome.html 样式 start*/
.dw-proof-of-income {
  margin-top: 44px;
  margin-bottom: 52px;
}

.dw-proof-of-income .dw-clean,
.dw-proof-of-income .dw-form {
  font-size: 18px;
  font-family: "PingFang SC Regular";
  color: #292929;
}

.dw-proof-of-income .dw-clean:active, .dw-proof-of-income .dw-clean:hover {
  color: #292929;
}

.dw-proof-of-income .dw-form .mui-input-row:after {
  height: 2px;
}

.dw-proof-of-income .dw-form .mui-input-row input[name="stampNum"],
.dw-proof-of-income .dw-form .mui-input-row input[name="bank"] {
  padding-right: 30px;
  float: right;
  width: 50%;
  color: #292929;
  text-align: right;
}

.dw-proof-of-income .dw-form .mui-input-row input[name="income"],
.dw-proof-of-income .dw-form .mui-input-row input[name="audit"],
.dw-proof-of-income .dw-form .mui-input-row input[name="auditCN"] {
  height: 22px;
  padding: 0 4px;
}

.dw-proof-of-income .dw-form .mui-input-row i {
  position: absolute;
  right: 0;
  height: 100%;
  font-size: 15px;
  font-weight: 600;
}

.dw-proof-of-income .dw-form .mui-input-row i:before {
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateX(-50%) translateY(-50%);
}

.dw-proof-of-income .dw-form .mui-input-row .dw-title {
  width: 135px;
  font-family: "PingFang SC Medium";
  padding-left: 13px;
  padding-right: 5px;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-title span {
  color: #f44719;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-mask-button {
  height: 100%;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: transparent;
  border: none;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-part-right {
  min-width: 175px;
  padding-top: 10px;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-part-right input {
  min-width: 92px;
  width: 120px;
  float: initial;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-apply-result {
  float: left;
  width: 56%;
  font-size: 18px;
  color: #292929;
  padding: 0 15px 0 0;
  margin-top: 10px;
  margin-bottom: 28px;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-apply-result:focus {
  border: 1px #999999 dotted;
}

.dw-proof-of-income .dw-form .mui-input-row .dw-word-total {
  font-size: 12px;
  color: #999999;
  text-align: right;
  line-height: 16px;
  margin-top: 5px;
  margin-right: 12px;
  position: absolute;
  right: 0;
  bottom: 0;
  width: 100%;
}

.dw-proof-of-income .dw-form .dw-input-top:after {
  height: 0;
}

.dw-proof-of-income .dw-form .dw-input-top label {
  line-height: 26px;
  padding-bottom: 0;
}

.dw-proof-of-income .dw-form .dw-input-top label ~ input {
  height: 35px;
  padding-bottom: 0;
}

.dw-proof-of-income .dw-form .dw-input-bottom label {
  line-height: 26px;
  padding-top: 0;
}

.dw-proof-of-income .dw-form .dw-input-bottom label ~ input {
  height: 35px;
  padding-top: 0;
}

.dw-proof-of-income .dw-form .dw-input-last:after {
  height: 0;
}

.dw-proof-of-income .dw-form .dw-exampl,
.dw-proof-of-income .dw-form .dw-note {
  width: 100%;
  line-height: 20px;
}

.dw-proof-of-income .dw-form .dw-exampl:after {
  height: 0;
}

.dw-proof-of-income .dw-form .dw-exampl input {
  text-align: center;
  width: 100%;
}

.dw-proof-of-income .dw-form .dw-note {
  position: relative;
  font-size: 12px;
  padding-left: 14px;
  padding-bottom: 10px;
  color: #999999;
}

.dw-proof-of-income .dw-form .dw-note:after {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 2px;
  content: '';
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5);
  background-color: #eaf0f3;
}

.dw-proof-of-income .mui-input-group input:focus {
  color: #292929;
  border: 1px #999999 dotted;
}

.dw-proof-of-income .dw-income-footer {
  margin-top: 10px;
}

.dw-proof-of-income .dw-income-footer .dw-tips {
  background: #eaf0f3;
  font-size: 12px;
  color: #666666;
  padding: 3px 5px;
}

.dw-proof-of-income .dw-income-footer .dw-tips span {
  height: 43px;
}

.dw-proof-of-income .dw-apply-submit {
  height: 51px;
  position: fixed;
  bottom: 0;
  width: 100%;
  z-index: 99;
  background: #fff;
  border-top: 1px #cccccc solid;
}

.dw-proof-of-income .dw-apply-submit .mui-btn {
  border: none;
  font-size: 21px;
  font-family: "PingFang SC Medium";
  width: 100%;
  height: 100%;
  color: #1B98E6;
}

/*apply-proofOfIncome.html 样式 end*/
/*Annoucement.html 样式 start*/
.dw-annoucement {
  display: flex;
  position: relative;
  padding-top: 0 !important;
  margin-top: 44px;
}

.dw-annoucement .new-ann-wrap {
  position: absolute;
  top: 0;
  background: #fff;
  width: 100%;
  height: 90px;
  margin-top: 10px;
}

.dw-annoucement .new-ann-wrap a {
  color: #292929;
  display: block;
  padding: 10px 18px 16px 72px;
  height: 100%;
}

.dw-annoucement .new-ann-wrap a .icon-annoucement-new {
  position: absolute;
  top: 0;
  left: 0;
  color: #1B98E6;
  font-size: 38px;
  height: 100%;
  line-height: 81px;
}

.dw-annoucement .new-ann-wrap a p {
  margin: 0;
}

.dw-annoucement .new-ann-wrap a p.title {
  font-family: "PingFang SC Regular";
  font-size: 15px;
  line-height: 20px;
  white-space: normal;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  word-break: break-all;
  -webkit-box-orient: vertical;
}

.dw-annoucement .new-ann-wrap a p.new-ann {
  font-family: "PingFang SC Medium";
  font-size: 18px;
  font-weight: bold;
  line-height: 24px;
}

.dw-annoucement .new-ann-wrap a p.date {
  text-align: right;
}

.dw-annoucement .notip-wrap {
  flex: auto;
  margin: 0 auto;
  text-align: center;
  padding-top: 88px;
  background: #fff;
}

.dw-annoucement .notip-wrap .svg-icon {
  font-size: 187px;
  height: 0.75em;
}

.dw-annoucement .notip-wrap p {
  color: #292929;
  font-size: 24px;
  margin-top: 10px;
}

/*Annoucement.html 样式 end*/
