		            <label>银行种类选择<i class="z-required">*</i></label>
		            <div class="cont">
		            	<span>{{$value.bankType | bankTypeText}}</span>
		            </div>
		        </div>
		        {{/if}}
		        <div class="mui-input-row">
		            <label>月均收入<i class="z-required">*</i></label>
		            <div class="cont">
